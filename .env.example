# LLM Provider Configuration
# Choose either "openai" or "anthropic"
MODEL_PROVIDER=openai

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-5-haiku-20241022

# Database Configuration (Optional)
# Override the default database path from config/db.yaml
# DUCKDB_PATH=./data/my_custom_database.duckdb

# Note: The system works without API keys for template-matched queries
# LLM is only used as a fallback for novel/ambiguous queries
